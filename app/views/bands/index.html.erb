<p id="notice"><%= notice %></p>

<h1>Listing Bands</h1>

<table>
  <thead>
    <tr>
      <th>Bn head</th>
      <th>Bn url</th>
      <th>Bn anonce</th>
      <th>Bn tema</th>
      <th>Bn rang</th>
      <th>Bn coment</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @bands.each do |band| %>
      <tr>
        <td><%= band.bn_head %></td>
        <td><%= band.bn_url %></td>
        <td><%= band.bn_anonce %></td>
        <td><%= band.bn_tema %></td>
        <td><%= band.bn_rang %></td>
        <td><%= band.bn_coment %></td>
        <td><%= link_to 'Show', band %></td>
        <td><%= link_to 'Edit', edit_band_path(band) %></td>
        <td><%= link_to 'Destroy', band, method: :delete, data: { confirm: 'Are you sure?' } %></td>
        <td><%#= link_to 'Browse', brose_band %></td>
        <td><%= link_to "Browse", band.bn_url %>  </td> 
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Band', new_band_path %>
